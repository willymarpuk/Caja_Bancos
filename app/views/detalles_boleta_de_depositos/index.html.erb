<h1>Listando detalle de boleta de deposito</h1>

<table>
  <thead>
    <tr>
      <th>Id boleta de deposito</th>
      <th>Efectivo</th>
      <th>Monto</th>
      <th>Item</th>
      <th>Id cheque entrante</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalles_boleta_de_depositos.each do |detalles_boleta_de_deposito| %>
      <tr>
        <td><%= detalles_boleta_de_deposito.id_boleta_de_deposito %></td>
        <td><%= detalles_boleta_de_deposito.efectivo %></td>
        <td><%= detalles_boleta_de_deposito.monto %></td>
        <td><%= detalles_boleta_de_deposito.item %></td>
        <td><%= detalles_boleta_de_deposito.id_cheque_entrante %></td>
        <td><%= link_to 'Mostrar', detalles_boleta_de_deposito %></td>
        <td><%= link_to 'Editar', edit_detalles_boleta_de_deposito_path(detalles_boleta_de_deposito) %></td>
        <td><%= link_to 'Destruir', detalles_boleta_de_deposito, method: :delete, data: { confirm: 'esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Detalle boleta de deposito', new_detalles_boleta_de_deposito_path %>
